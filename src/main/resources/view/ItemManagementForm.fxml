<AnchorPane prefHeight="435.0" prefWidth="707.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.itemmanagement.ItemFormController">
   <children>
      <TableView fx:id="tblItemManagement" layoutX="74.0" layoutY="79.0" prefHeight="278.0" prefWidth="560.0">
        <columns>
          <TableColumn fx:id="colItemCode" prefWidth="75.0" text="Code" />
          <TableColumn fx:id="colDescription" prefWidth="217.0" text="Description" />
            <TableColumn fx:id="colPackSize" prefWidth="88.0" text="PackSize" />
            <TableColumn fx:id="colUnitPrice" prefWidth="89.0" text="UnitPrice" />
            <TableColumn fx:id="colQtyOnHand" prefWidth="90.0" text="QtyOnHand" />
