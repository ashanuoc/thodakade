            <JFXButton fx:id="btnUpdate" buttonType="RAISED" onAction="#btnUpdateOnAction" prefHeight="35.0" prefWidth="105.0" text="UPDATE">
               <font>
                  <Font size="16.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btnDelete" buttonType="RAISED" onAction="#btnDeleteOnAction" prefHeight="35.0" prefWidth="134.0" text="DELETE">
               <font>
                  <Font size="16.0" />
               </font>
            </JFXButton>
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
