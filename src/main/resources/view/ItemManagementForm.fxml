            <Font size="26.0" />
         </font>
      </Label>
      <ButtonBar layoutX="143.0" layoutY="364.0" prefHeight="40.0" prefWidth="421.0">
        <buttons>
            <JFXButton fx:id="btinView" buttonType="RAISED" onAction="#btinViewOnAction" text="VIEW">
